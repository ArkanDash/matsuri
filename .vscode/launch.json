    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch General",
            "cwd": "${workspaceFolder}/services/general",
            "program": "${workspaceFolder}/services/general/src/index.ts",
            "request": "launch",
            "envFile": "${workspaceFolder}/services/general/.env",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "preLaunchTask": "npm: compile general",
            "type": "node",
            "sourceMaps": true,
            "outputCapture": "std"
        },
    ]
}